<h3 class="text-dark">Our Team</h3>
<div class="row">
  <div class="row">
    {{#each data}}
      <div class="col-md-3">
        <a
          href="#"
          onclick="loadData('team/{{id}}', '#content', 'aboutus-details');"
        >
          <img src="{{thumbpath}}" alt="" class="img-rounded" />
        </a>
        <a
          href="#"
          onclick="loadData('team/{{id}}', '#content', 'aboutus-details');"
        >
          <p class="text-dark font-weight-bold pt-3">{{name}}</p>
        </a>
        <p>{{{summary}}}</p>
      </div>
    {{/each}}
  </div>
</div>